@model IEnumerable<ShopApplication.Models.BrandDto>

<h2 class="text-center">برندهای محصولات</h2>
<br />
<section id="demos">
    <div class="row">
        <div class="owl-carousel owl-theme">
            @foreach (var item in Model)
            {
                <div class="item">
                    <img width="100" height="115" src="~/Images/Brands/@item.ImageName" alt="@item.Title" />
                </div>
            }

        </div>
    </div>
</section>

